{{!
	Table of Contents template for Guanches skin
}}
<nav class="guanches-toc" id="toc" aria-label="{{msg-toc}}">
	<div class="guanches-toc-header">
		<h2 class="guanches-toc-title">{{msg-toc}}</h2>
		<button class="guanches-toc-toggle" aria-label="{{msg-hidetoc}}" aria-expanded="true" aria-controls="toc-content">{{msg-toc}}</button>
	</div>
	<div class="guanches-toc-content" id="toc-content">
		{{{html-toc}}}
	</div>
</nav>