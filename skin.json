{
	"name": "Guanches",
	"version": "1.0.0",
	"author": [
		"Eduardo Nacimiento-GarcÃ­a"
	],
	"url": "https://guanches.org",
	"descriptionmsg": "guanches-skin-desc",
	"namemsg": "skinname-guanches",
	"license-name": "GPL-3.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.45.0"
	},
	"ValidSkinNames": {
		"guanches": {
			"class": "MediaWiki\\Skins\\Guanches\\SkinGuanches",
			"args": [
				{
					"name": "guanches",
					"responsive": true,
					"toc": true,
					"messages": [
						"sitetitle",
						"search",
						"otherlanguages",
						"tagline",
						"toolbox",
						"navigation",
						"guanches-skin-desc"
					],
					"styles": [
						"mediawiki.skinning.interface",
						"codex-styles",
						"mediawiki.ui.icon",
						"mediawiki.ui.button",
						"skins.guanches.styles"
					],
					"scripts": [
						"skins.guanches"
					],
					"bodyClasses": [
						"skin-guanches",
						"mw-body"
					],
					"version": "1.0.0"
				}
			]
		}
	},
	"AutoloadClasses": {
		"MediaWiki\\Skins\\Guanches\\SkinGuanches": "src/SkinGuanches.php"
	},
	"MessagesDirs": {
		"Guanches": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.guanches.styles": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content-tables": true,
				"content-links": true,
				"content-media": true,
				"content-links-external": true,
				"interface-message-box": true,
				"interface-category": true,
				"toc": true
			},
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": [
				"resources/guanches.css"
			]
		},
		"skins.guanches": {
			"targets": [
				"desktop",
				"mobile"
			],
			"packageFiles": [
				"resources/guanches.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Guanches"
	},
	"ResourceModuleSkinStyles": {
		"guanches": {
			"common": [
				"skins.guanches.styles"
			]
		}
	},
	"manifest_version": 2
}